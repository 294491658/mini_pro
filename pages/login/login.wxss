/* pages/login/login.wxss */
.center{
  text-align: center;
  margin-top: 30%;
}
image{
  width: 69%;
  height: 161rpx;
  margin-bottom: 40rpx;
}
button{
  margin: 0 auto;
  margin-top: 50rpx; 
  width: 80%;
  background-color: rgb(0, 155, 76);
  color: white;
}

.userPwd,.userName{
  margin: 0 auto;
  width: 80%;
  display:block;
  height:1.4rem;
  text-overflow:clip;
  overflow:hidden;
  white-space:nowrap;
  font-family:UICTFontTextStyleBody;
  min-height:1.4rem;
  background-color: rgb(241, 241, 241);
  margin-top: 30rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  text-align: center;
  border-radius: 8rpx;
}
.remember{
  margin: 14px;
}
.checkbox {
  color: rgba(156, 150, 150, 0.863)
}
checkbox{
  transform:scale(0.7);
}
 