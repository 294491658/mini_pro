/* 分页样式 */
.view-page{
  margin: 0 auto;
  width: 80%;
  height: 80px;
  text-align: center;
  margin-top: 20px;
  margin-bottom: 20px;
  flex-direction:column;
}
.view-page view{
  float: left;
}
.btn{
  background-color: rgb(0, 155, 76);
  color: white;
  border-radius: 12px;
  height: 25px;
  line-height: 25px;
}
.search{
  width: 98%;
  height: 40px;
  line-height: 40px;
  border-radius: 22px;
  background-color: rgb(241, 241, 241);
  margin: 0 auto;
  margin-top: 10px;
}
